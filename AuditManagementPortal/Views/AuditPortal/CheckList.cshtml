@model IEnumerable<AuditManagementPortal.Models.CheckListQuestion>

@{
    ViewData["Title"] = "CheckList";
}

<h1>Questions</h1>

<table class="table">
    <thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.QuestionNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Question)
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.QuestionNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Question)
            </td>
        </tr>
    }
    </tbody>
</table>

<div>
    <a asp-action="Home">Back to Home</a>
</div>
